public class MovablePoint implements Movable {
  private int x;
  private int y;
  
  public MovablePoint(int a, int b){
    x = a;
    y=b;
  }
  public int getx(){
    return x;
  }
  public int gety() {
    return y;
  }
  public String toString() {
   return "( "+x+", " +y+" )"; 
  }
  
  public void moveUp(){
     y+=1;
  }

  public void moveDown(){
    y-=1;
  }



  public void moveLeft() {x-=1;}



public void moveRight() {x+=1;}
  
}
